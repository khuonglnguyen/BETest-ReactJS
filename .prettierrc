{
  "arrowParens": "avoid",
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxBracketSameLine": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
