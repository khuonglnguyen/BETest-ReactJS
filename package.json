{
  "name": "be-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "start:release": "env-cmd",
    "build:release": "env-cmd",
    "test": "react-app-rewired test",
    "flow": "flow",
    "lint": "eslint --ext js,jsx,ts,tsx src/",
    "lint:fix": "eslint --fix --ext js,jsx,ts,tsx src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "start:dev": "env-cmd -f development.env npm run start",
    "build:dev": "env-cmd -f development.env npm run build"
  },
  "dependencies": {
    "@ag-grid-community/react": "^28.2.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@mui/styled-engine-sc": "^5.8.0",
    "@reduxjs/toolkit": "^1.8.3",
    "ag-grid-community": "^28.2.0",
    "ag-grid-react": "^28.2.0",
    "ajv": "^8.11.0",
    "ajv-keywords": "^5.1.0",
    "antd": "^4.21.5",
    "array-move": "^4.0.0",
    "axios": "^0.27.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-relay": "^14.0.0",
    "bootstrap": "^5.1.3",
    "clsx": "^1.1.1",
    "customize-cra": "^1.0.0",
    "dragula": "^3.7.3",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "immutability-helper": "^3.1.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-drag-listview": "^0.2.1",
    "react-loadable": "^5.5.0",
    "react-overlays": "^5.2.0",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.0.5",
    "reactjs-popup": "^2.0.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass": "^1.53.0",
    "styled-components": "^5.3.5",
    "tableau-api": "^2.2.3",
    "typescript": "^4.7.4",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "!src/reducers/**/index.tsx",
      "src/actions/**/*.tsx",
      "src/reducers/**/*.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.42",
    "@types/react": "^18.0.14",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.25",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^3.6.0",
    "tslint-react-hooks": "^2.2.2"
  }
}
